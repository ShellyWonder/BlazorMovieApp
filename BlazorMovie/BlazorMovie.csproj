<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
	  <NoWarn>NU1902,NU1901</NoWarn>
	  <UserSecretsId>a3d592a7-e88a-4756-9e9d-e3845abe3797</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Components\Layout\NewFolder\**" />
    <Content Remove="Components\Layout\NewFolder\**" />
    <EmbeddedResource Remove="Components\Layout\NewFolder\**" />
    <None Remove="Components\Layout\NewFolder\**" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="8.0.10" />
    <PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="8.0.10" PrivateAssets="all" />
  </ItemGroup>

</Project>
